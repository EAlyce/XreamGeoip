{
  "input": [
    {
      "type": "text",
      "action": "add",
      "args": {
        "inputDir": "./data"
      }
    },
    {
      "type": "maxmindMMDB",
      "action": "add",
      "args": {
        "uri": "https://raw.githubusercontent.com/alecthw/mmdb_china_ip_list/release/Country.mmdb",
        "wantedList": [
          "cn"
        ]
      }
    },
    {
      "type": "private",
      "action": "add"
    }
  ],
  "output": [
    {
      "type": "v2rayGeoIPDat",
      "action": "output",
      "args": {
        "outputName": "ipinfo.geoip.dat"
      }
    },
    {
      "type": "v2rayGeoIPDat",
      "action": "output",
      "args": {
        "outputName": "ipinfo.geoip-only-cn-private.dat",
        "wantedList": [
          "cn",
          "private"
        ]
      }
    },
    {
      "type": "maxmindMMDB",
      "action": "output",
      "args": {
        "outputName": "ipinfo.country.mmdb"
      }
    },
    {
      "type": "maxmindMMDB",
      "action": "output",
      "args": {
        "outputName": "ipinfo.country-only-cn-private.mmdb",
        "wantedList": [
          "cn",
          "private"
        ]
      }
    }
  ]
}